<body>
    <main class="flex-grow-1 d-flex align-items-center justify-content-center flex-column p-5">
    <form class="mt-3 bg-info p-5 rounded-3" style="--bs-bg-opacity: .2;width: 600px ;">
            <!-- Formulario para recopilar los datos personales -->
        <h1 class="text-center  text-white">datos personales</h1>
        <div class="row">
            <!-- Campo para el primer nombre -->
            <div class="col-md-6">
                <label for="inputFirstName" class="form-label text-white">Primer nombre</label>
                <div class="input-group mb-3">
                    <span class="input-group-text  bg-primary-subtle" id="basic-addon1">@</span>
                    <input type="text" class="form-control bg-primary-subtle" aria-describedby="basic-addon1" id="nombre1" placeholder="<%= datosUsuario.nombre1 %>" disabled>
                </div>
            </div>
            <!-- Campo para el segundo nombre -->
            <div class="col-md-6">
                <label for="inputFirstName" class="form-label text-white">segundo nombre</label>
                <div class="input-group mb-3">
                    <span class="input-group-text  bg-primary-subtle" id="basic-addon2">@</span>
                    <input type="text" class="form-control bg-primary-subtle" aria-describedby="basic-addon2" id="nombre2" placeholder="<%= datosUsuario.nombre2 %>" disabled>
                </div>
            </div>
        </div>
        <div class="row">
            <!-- Campo para el primer apellido -->
            <div class="col-md-6">
                <label for="inputFirstName" class="form-label   text-white">primer apellido</label>
                <div class="input-group mb-3">
                    <span class="input-group-text  bg-primary-subtle" id="basic-addon3">@</span>
                    <input type="text" class="form-control bg-primary-subtle" aria-describedby="basic-addon3" id="apellido1" placeholder="<%= datosUsuario.apellido1 %>" disabled>
                </div>
            </div>
            <!-- Campo para el segundo apellido -->
            <div class="col-md-6">
                <label for="inputFirstName" class="form-label  text-white">segundo apellido</label>
                <div class="input-group mb-3">
                    <span class="input-group-text  bg-primary-subtle" id="basic-addon4">@</span>
                    <input type="text" class="form-control bg-primary-subtle" aria-describedby="basic-addon4" id="apellido2" placeholder="<%= datosUsuario.apellido2 %>" disabled>
                </div>
            </div>
        </div>
        <!-- Campo para el telefono -->
        <div class="row">
            <div class="col-md-6">
                <label for="inputFirstName" class="form-label   text-white">telefono</label>
                <div class="input-group mb-3">
                    <span class="input-group-text  bg-primary-subtle" id="basic-addon3">@</span>
                    <input type="text" class="form-control  bg-primary-subtle"  aria-describedby="basic-addon3" id="telefono" placeholder="<%= datosUsuario.telefono %>" disabled>
                </div>
            </div>
            <!-- Campo para el fecha de nacimiento -->
            <div class="col-md-6">
                <label for="inputFirstName" class="form-label   text-white">fecha de nacimiento</label>
                <div class="input-group mb-3">
                    <span class="input-group-text bg-primary-subtle" id="basic-addon4">@</span>
                    <input type="text" class="form-control bg-primary-subtle" aria-describedby="basic-addon4" id="fecha_nacimiento" placeholder="<%= datosUsuario.fecha_nacimiento %>" disabled>
                </div>
            </div>
        </div>
        <!-- Campo para el tipo de sangre -->
        <div class="row">
            <div class="col-md-6">
                <label for="inputFirstName" class="form-label  text-white">tipo de sangre</label>
                <div class="input-group mb-3">
                    <span class="input-group-text  bg-primary-subtle" id="basic-addon3">@</span>
                    <input type="text" class="form-control  bg-primary-subtle"  aria-describedby="basic-addon3" id="tipo_de_sangre" placeholder="<%= datosUsuario.tipo_de_sangre %>" disabled>
                </div>
            </div>
            <!-- Campo para el direccion -->
            <div class="col-md-6">
                <label for="inputFirstName" class="form-label  text-white">direccion</label>
                <div class="input-group mb-3">
                    <span class="input-group-text  bg-primary-subtle" id="basic-addon4">@</span>
                    <input type="text" class="form-control  bg-primary-subtle" aria-describedby="basic-addon4" id="direccion" placeholder="<%= datosUsuario.direccion %>" disabled >
                </div>
            </div>
        </div>
        <!-- Campo para el email -->
        <div class="row">
            <div class="col-md-6">
                <label for="inputFirstName" class="form-label  text-white">email</label>
                <div class="input-group mb-3">
                    <span class="input-group-text  bg-primary-subtle" id="basic-addon3">@</span>
                    <input type="text" class="form-control  bg-primary-subtle"  aria-describedby="basic-addon3" id="correo" placeholder="<%= datosUsuario.correo %>" disabled>
                </div>
            </div>
            <!-- Campo para el ciudad -->
            <div class="col-md-6">
                <label for="inputFirstName" class="form-label  text-white">ciudad</label>
                <div class="input-group mb-3">
                    <span class="input-group-text  bg-primary-subtle" id="basic-addon4">@</span>
                    <input type="text" class="form-control bg-primary-subtle"  aria-describedby="basic-addon4" id="ciudad" placeholder="<%= datosUsuario.ciudad %>" disabled>
                </div>
            </div>
        </div>
        <!-- Campo para el identificacion -->
        <div class="row">
            <div class="col-md-6">
                <label for="inputFirstName" class="form-label  text-white">identificacion</label>
                <div class="input-group mb-3">
                    <span class="input-group-text  bg-primary-subtle" id="basic-addon3">@</span>
                    <input type="text" class="form-control bg-primary-subtle" aria-describedby="basic-addon3" id="identificacion" placeholder="<%= datosUsuario.identificacion %>" disabled>
                </div>
            </div>
            <!-- Campo para el ocupacion -->
            <div class="col-md-6">
                <label for="inputFirstName" class="form-label text-white">ocupacion</label>
                <div class="input-group mb-3">
                    <span class="input-group-text   bg-primary-subtle" id="basic-addon4">@</span>
                    <input type="text" class="form-control  bg-primary-subtle"  aria-describedby="basic-addon4" id="ocupacion" placeholder="<%= datosUsuario.ocupacion %>" disabled>
                </div>
            </div>
        </div>

    </form> <!-- Fin del formulario -->

</main><!-- Fin del contenedor principal -->

